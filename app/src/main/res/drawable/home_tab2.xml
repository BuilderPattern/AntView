<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 没有焦点时 -->
    <item android:drawable="@mipmap/tab_manage_normal" android:state_selected="false" />
    <!--选中时的 -->
    <item android:drawable="@mipmap/tab_manage_pressed" android:state_selected="true" />
    <!-- 非触摸模式下获得焦点并单击 -->
    <item android:drawable="@mipmap/tab_manage_pressed" android:state_focused="true" android:state_pressed="true" />


</selector>